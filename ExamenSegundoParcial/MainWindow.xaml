<Window x:Class="ExamenSegundoParcial.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExamenSegundoParcial"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="800">
    <StackPanel>
		<TextBlock TextAlignment="Center" Margin="10" FontFamily="Adobe Gothic Std B" FontSize="24">1.Informacion Personal 2. Contacto 3.Informacion De Pago</TextBlock>
		<local:Contacto x:Name="ControlContacto" Visibility="Collapsed"></local:Contacto>
		<local:Informacion_De_Pago x:Name="ControlInformacionPago" Visibility="Collapsed"></local:Informacion_De_Pago>
		<local:Informacion_Personal x:Name="ControlInformacionPersonal" Visibility="Visible"></local:Informacion_Personal>
		<TextBlock Name="Error" TextAlignment="Right" Margin="10" FontFamily="Adobe Gothic Std B"></TextBlock>
		<Button HorizontalAlignment="Right" Click="Button_Click" Margin="10" FontFamily="Adobe Gothic Std B" FontSize="24">Siguente</Button>
	</StackPanel>
</Window>
