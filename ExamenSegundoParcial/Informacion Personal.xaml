<UserControl x:Class="ExamenSegundoParcial.Informacion_Personal"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ExamenSegundoParcial"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
		<TextBlock TextAlignment="Left" Margin="10" FontFamily="Adobe Gothic Std B" FontSize="24">Nombre:</TextBlock>
		<TextBox Name="Nombre" FontFamily="Adobe Gothic Std B" FontSize="24"></TextBox>
		<TextBlock TextAlignment="Left" Margin="10" FontFamily="Adobe Gothic Std B" FontSize="24">Apellido Paterno:</TextBlock>
		<TextBox  Name="ApellidoPaterno" FontFamily="Adobe Gothic Std B" FontSize="24"></TextBox>
		<TextBlock TextAlignment="Left" Margin="10" FontFamily="Adobe Gothic Std B" FontSize="24">Apellido Materno:</TextBlock>
		<TextBox Name="ApellidoMaterno"  FontFamily="Adobe Gothic Std B" FontSize="24"></TextBox>
		<TextBlock Name="Edad" TextAlignment="Left" Margin="10" FontFamily="Adobe Gothic Std B" FontSize="24">Edad:</TextBlock>
		<TextBox FontFamily="Adobe Gothic Std B" FontSize="24"></TextBox>
	</StackPanel>
</UserControl>
