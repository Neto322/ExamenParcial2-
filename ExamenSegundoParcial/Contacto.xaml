<UserControl x:Class="ExamenSegundoParcial.Contacto"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ExamenSegundoParcial"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
		<TextBlock Name="CoRREO" TextAlignment="Left" Margin="10" FontFamily="Adobe Gothic Std B" FontSize="24">Correo:</TextBlock>
		<TextBox  FontFamily="Adobe Gothic Std B" FontSize="24"></TextBox>
		<TextBlock Name="Telefono" TextAlignment="Left" Margin="10" FontFamily="Adobe Gothic Std B" FontSize="24"> Telefono:</TextBlock>
		<TextBox FontFamily="Adobe Gothic Std B" FontSize="24"></TextBox>
		<TextBlock Name="Celular" TextAlignment="Left" Margin="10" FontFamily="Adobe Gothic Std B" FontSize="24"> Celular:</TextBlock>
		<TextBox FontFamily="Adobe Gothic Std B" FontSize="24"></TextBox>
	</StackPanel>
</UserControl>
